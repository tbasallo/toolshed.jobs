<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFrameworks>net10.0</TargetFrameworks>
		<AssemblyName>Toolshed.Jobs</AssemblyName>
		<Description>An Azure table storage based job tracking library. Allows for logging of events and steps.</Description>
		<PackageProjectUrl>https://github.com/tbasallo/toolshed.jobs</PackageProjectUrl>
		<Version>10.0.0</Version>
		<RepositoryUrl>https://github.com/tbasallo/toolshed.jobs</RepositoryUrl>
		<RepositoryType>GIT</RepositoryType>
		<Authors>Tony Basallo</Authors>
		<Nullable>enable</Nullable>
		<ImplicitUsings>enable</ImplicitUsings>
	</PropertyGroup>

	<ItemGroup>
		<PackageReference Include="Azure.Data.Tables" Version="12.11.0" />
		<PackageReference Include="Azure.Storage.Queues" Version="12.25.0" />
		<PackageReference Include="Toolshed" Version="10.0.0" />
		<PackageReference Include="Toolshed.AzureStorage" Version="10.0.0" />
	</ItemGroup>

</Project>
